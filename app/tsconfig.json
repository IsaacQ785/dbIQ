{
  "compilerOptions": {
    "types": ["node"],
    "lib":["DOM", "ESNext"],
    "importHelpers": true,
    "rootDir": "./",
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es6",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true
  },
  // "include": ["./src", ".eslintrc.js","./webpack", "serveApp.js"]
}